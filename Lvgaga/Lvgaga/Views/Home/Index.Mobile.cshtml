@using LvModel.Common
@using LvService.Common
@model LvModel.View.Tumblr.TumblrsModel
@{
    ViewBag.Title = "首页";
}

<div>
    @Html.React("TumblrBoxMobile", new
    {
        dataContext = Model,
        tableNameOfFavorite = LvConstants.TableNameOfFavorite,
        tableNameOfTumblr = LvConstants.TableNameOfTumblr,
        tableNameOfComment = LvConstants.TableNameOfComment,
        host = Request.Url != null ? Request.Url.DnsSafeHost : LvConfiguration.DefaultHostName
    })
</div>

@section scripts
{
    @Scripts.Render("~/bundles/home/mobile")
    @Html.ReactInitJavaScript()
}